DataFrameStyler	dataframestylerClass.py	/^class DataFrameStyler:$/;"	c
__init__	dataframestylerClass.py	/^    def __init__(self, df: pd.DataFrame):$/;"	m	class:DataFrameStyler
color_cells	dataframestylerClass.py	/^    def color_cells(self, color: str = 'yellow', condition=None) -> 'DataFrameStyler':$/;"	m	class:DataFrameStyler
color_cells	dataframestylerMethods.py	/^def color_cells(df: pd.DataFrame, color: str = 'yellow', condition=None) -> pd.DataFrame:$/;"	f
pd	dataframestylerClass.py	/^import pandas as pd$/;"	i
pd	dataframestylerMethods.py	/^import pandas as pd$/;"	i
render	dataframestylerClass.py	/^    def render(self) -> None:$/;"	m	class:DataFrameStyler
style_font	dataframestylerClass.py	/^    def style_font(self, fontsize: int = 12, fontweight: str = 'normal', fontstyle: str = 'normal') -> 'DataFrameStyler':$/;"	m	class:DataFrameStyler
style_font	dataframestylerMethods.py	/^def style_font(df: pd.DataFrame, fontsize: int = 12, fontweight: str = 'normal', fontstyle: str = 'normal') -> pd.DataFrame:$/;"	f
styled_df	dataframestylerMethods.py	/^styled_df = (df.style$/;"	v
